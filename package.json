{
  "name": "@s3xysteak/utils",
  "type": "module",
  "version": "0.9.0",
  "private": false,
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "description": "s3xysteak/utils",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://github.com/s3xysteak/utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3xysteak/utils.git"
  },
  "bugs": "https://github.com/s3xysteak/utils/issues",
  "keywords": [
    "type",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./import": {
      "import": {
        "types": "./dist/import.d.ts",
        "default": "./dist/import.mjs"
      },
      "require": {
        "types": "./dist/import.d.ts",
        "default": "./dist/import.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "esno ./scripts/update-import.ts && tsdown",
    "test": "vitest",
    "test:once": "vitest run",
    "release": "tsc && bumpp",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0",
    "auto-import-helper": "^0.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@types/node": "^24.3.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "esno": "^4.8.0",
    "tsdown": "^0.15.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
