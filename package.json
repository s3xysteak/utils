{
  "name": "@s3xysteak/utils",
  "type": "module",
  "version": "0.7.3",
  "private": false,
  "description": "s3xysteak/utils",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://github.com/s3xysteak/utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3xysteak/utils.git"
  },
  "bugs": "https://github.com/s3xysteak/utils/issues",
  "keywords": [
    "type",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "import": {
        "types": "./dist/resolver.d.ts",
        "default": "./dist/resolver.mjs"
      },
      "require": {
        "types": "./dist/resolver.d.ts",
        "default": "./dist/resolver.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && unbuild",
    "build-only": "unbuild",
    "test": "vitest",
    "test-once": "vitest run",
    "release": "tsc && esno ./scripts/release.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@types/node": "^20.14.9",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "unplugin-export-collector": "^0.5.0",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
